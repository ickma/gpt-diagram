{
    "compilerOptions": {
        "baseUrl": "./src",
        "paths": {
          "*":["*"]
        },
        "outDir": "./public/",
        "sourceMap": true,
        "strictNullChecks": true,
        "module": "es6",
        "jsx": "react-jsx",
        "jsxImportSource": "@emotion/react",
        "target": "es5",
        "allowJs": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
    },
    "include": [
      "src/**/*.ts",
      "src/**/*.js",
      "src/**/*.tsx",
      "src/**/*.jsx",
      "src/**/*.d",
      "src/**/*.d.js",
    ],
  }